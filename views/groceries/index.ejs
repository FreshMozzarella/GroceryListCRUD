<%- include('../partials/header') %>
<h2>All Groceries</h2>
<table id="list">
  <thead>
    <tr>
      <th>Foods</th>
      <th>Brands</th>
      <th>Quantity</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>

    <% groceries.forEach(function(g) { %>
    <tr>
      <td><%= g.foods %></td>
      <!-- <td><%= g.brands %></td> -->
      <td><%= g.quantity %></td>
      <!-- <td><%= g.price %></td> -->
    </tr>
    <%}) %>
  </tbody>
</table>
<%- include('../partials/footer') %>